#ifndef CARRAY_HPP_
#define CARRAY_HPP_

template<typename T, unsigned int N>
class CArray{
private:
	T[N] entries;
public:
	CArray(){
		entries = new T[N];
	}
	~CArray();
	T& operator[](unsigned int index);
};


#endif /* CARRAY_HPP_ */

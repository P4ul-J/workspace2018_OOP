#include "CCounter.hpp"

CCounter::CCounter(): m_value(0){
}

CCounter::~CCounter(){
}

int CCounter::getValue()const{
	return m_value;
}

void CCounter::setValue(int value){
	m_value = value;
}

void CCounter::count(){
}
bool CCounter::operator>(CCounter const& left, CCounter const& right){
	if(left.m_value > right.m_value)return true;
	else return false;
}
bool CCounter::operator<(CCounter const& left, CCounter const& right){
	if(left.m_value < right.m_value)return true;
	else return false;
}

